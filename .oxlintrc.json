{
	"$schema": "./node_modules/oxlint/configuration_schema.json",
	"ignorePatterns": ["./@types/generated/**"],
	"categories": {
		"correctness": "error",
		"pedantic": "error",
		"perf": "warn",
		"suspicious": "error"
	},
	"env": {
		"browser": true
	},
	"globals": {},
	"plugins": ["import", "jest", "oxc", "typescript", "unicorn", "vitest"],
	"settings": {},
	"rules": {
		"consistent-empty-array-spread": "error",
		"escape-case": "error",
		"eslint/array-callback-return": "error",
		"eslint/default-case-last": "error",
		"eslint/default-param-last": "error",
		"eslint/eqeqeq": "error",
		"eslint/guard-for-in": "error",
		"eslint/max-lines": "off",
		"eslint/max-depth": ["warn", 4],
		"eslint/max-nested-callbacks": ["error", { "max": 3 }],
		"eslint/max-lines-per-function": "off",
		"eslint/max-params": ["error", { "max": 3 }],
		"eslint/max-classes-per-file": "off",
		"eslint/new-cap": "error",
		"eslint/no-array-constructor": "error",
		"eslint/no-case-declarations": "error",
		"eslint/no-console": "warn",
		"eslint/no-constructor-return": "error",
		"eslint/no-div-regex": "warn",
		"eslint/no-duplicate-imports": "error",
		"eslint/no-else-return": "error",
		"eslint/no-empty": "error",
		"eslint/no-empty-function": ["error", { "allow": ["constructors"] }],
		"eslint/no-eq-null": "error",
		"eslint/no-eval": "error",
		"eslint/no-fallthrough": "error",
		"eslint/no-inner-declarations": "error",
		"eslint/no-lone-blocks": "error",
		"eslint/no-multi-assign": "error",
		"eslint/no-multi-str": "error",
		"eslint/no-negated-condition": "warn",
		"eslint/no-nested-ternary": "error",
		"eslint/no-new-func": "error",
		"eslint/no-new-wrappers": "error",
		"eslint/no-object-constructor": "error",
		"eslint/no-proto": "error",
		"eslint/no-prototype-builtins": "error",
		"eslint/no-regex-spaces": "error",
		"eslint/no-return-assign": "error",
		"eslint/no-script-url": "error",
		"eslint/no-self-compare": "error",
		"eslint/no-template-curly-in-string": "error",
		"eslint/no-throw-literal": "error",
		"eslint/no-unused-expressions": "error",
		"eslint/no-var": "error",
		"eslint/prefer-exponentiation-operator": "error",
		"eslint/prefer-object-has-own": "error",
		"eslint/prefer-object-spread": "error",
		"eslint/prefer-promise-reject-errors": "error",
		"eslint/prefer-rest-params": "error",
		"eslint/prefer-spread": "off",
		"eslint/radix": "error",
		"eslint/require-await": "error",
		"eslint/symbol-description": "error",
		"eslint/yoda": [
			"error",
			{
				"exceptRange": true
			}
		],
		"explicit-length-check": "error",
		"import/export": "error",
		"import/first": "error",
		"import/max-dependencies": "off",
		"import/no-amd": "error",
		"import/no-commonjs": "error",
		"import/no-cycle": [
			"error",
			{
				"maxDepth": 1
			}
		],
		"import/no-default-export": "warn",
		"import/no-dynamic-require": "error",
		"import/no-namespace": "error",
		"import/no-webapck-loader-syntax": "error",
		"new-for-builtins": "error",
		"no-hex-escape": "error",
		"no-instanceof-array": "error",
		"no-lonely-if": "error",
		"no-negation-in-equality-check": "error",
		"no-new-buffer": "error",
		"no-static-only-class": "error",
		"no-this-assignment": "error",
		"no-typeof-undefined": "error",
		"no-unreadable-array-destructuring ": "error",
		"no-unreadable-iife": "error",
		"no-useless-promise-resolve-reject": "error",
		"no-useless-switch-case": "error",
		"no-useless-undefined": "error",
		"oxc/bad-bitwise-operator": "error",
		"oxc/no-barrel-file": "error",
		"oxc/no-const-enum": "error",
		"oxc/no-map-spread": "error",
		"promise/avoid-new": "error",
		"promise/catch-or-return": "error",
		"promise/no-return-in-finally": "error",
		"promise/param-names": "error",
		"promise/prefer-await-to-callbacks": "error",
		"promise/prefer-await-to-then": "error",
		"typescript/adjacent-overload-signatures": "error",
		"typescript/array-type": "error",
		"typescript/ban-ts-comment": "error",
		"typescript/ban-types": "error",
		"typescript/consistent-generic-constructors": "error",
		"typescript/consistent-indexed-object-style": "error",
		"typescript/consistent-type-imports": "error",
		"typescript/explicit-function-return-type": "warn",
		"typescript/no-dynamic-delete": "error",
		"typescript/no-empty-interface": "error",
		"typescript/no-empty-object-type": "error",
		"typescript/no-explicit-any": "error",
		"typescript/no-import-type-side-effects": "error",
		"typescript/no-inferrable-types": "error",
		"typescript/no-non-null-assertion": "error",
		"typescript/no-require-imports": "error",
		"typescript/no-unsafe-function-type": "error",
		"typescript/no-var-requires": "error",
		"typescript/prefer-for-of": "error",
		"typescript/prefer-function-type": "error",
		"typescript/prefer-ts-expect-error": "error",
		"unicorn/catch-error-name": "error",
		"unicorn/consistent-existence-index-check": "error",
		"unicorn/error-message": "error",
		"unicorn/filename-case": [
			"error",
			{
				"cases": {
					"camelCase": false,
					"kebabCase": true,
					"pascalCase": false,
					"snakeCase": false
				}
			}
		],
		"unicorn/no-abusive-eslint-disable": "error",
		"unicorn/no-array-for-each": "error",
		"unicorn/no-array-reduce": "off",
		"unicorn/no-await-expression-member": "error",
		"unicorn/no-length-as-slice-end": "error",
		"unicorn/no-magic-array-flat-depth": "error",
		"unicorn/numeric-separators-style": "warn",
		"unicorn/prefer-array-flat": "error",
		"unicorn/prefer-array-flat-map": "error",
		"unicorn/prefer-array-some": "error",
		"unicorn/prefer-blob-reading-methods": "error",
		"unicorn/prefer-code-point": "error",
		"unicorn/prefer-date-now": "error",
		"unicorn/prefer-dom-node-dataset": "error",
		"unicorn/prefer-includes": "error",
		"unicorn/prefer-logical-operator-over-ternary": "warn",
		"unicorn/prefer-math-min-max": "error",
		"unicorn/prefer-math-trunc": "error",
		"unicorn/prefer-modern-dom-apis": "error",
		"unicorn/prefer-modern-math-apis": "error",
		"unicorn/prefer-native-coercion-functions": "error",
		"unicorn/prefer-negative-index": "error",
		"unicorn/prefer-node-protocol": "error",
		"unicorn/prefer-number-properties": "error",
		"unicorn/prefer-regexp-test": "error",
		"unicorn/prefer-spread": "off",
		"unicorn/prefer-string-raw": "error",
		"unicorn/prefer-string-replace-all": "error",
		"unicorn/prefer-string-slice": "error",
		"unicorn/prefer-string-trim-start-end": "error",
		"unicorn/prefer-structured-clone": "error",
		"unicorn/prefer-type-error": "error",
		"unicorn/require-array-join-separator": "error",
		"unicorn/require-number-to-fixed-digits-argument": "error",
		"unicorn/switch-case-braces": "error",
		"unicorn/throw-new-error": "error",
		"unicorn/unicorn/no-nul": "error",
		"unicorn/prevent-abbreviation": "error"
	},
	"overrides": [
		{
			"files": [
				"src/app.tsx",
				"src/entry-client.tsx",
				"src/entry-server.tsx",
				"src/routes/**/*.tsx"
			],
			"rules": {
				"import/no-default-export": "off"
			}
		},
		{
			"files": ["*.spec.ts", "*.test.ts"],
			"rules": {
				"expect-expect": [
					"error",
					{
						"assertFunctionNames": ["assert", "expect"]
					}
				],
				"unicorn/no-new-array": "off",
				"eslint/max-params": "off",
				"eslint/no-array-constructor": "off",
				"max-nested-describe": "error",
				"no-conditional-in-test": "error",
				"eslint/no-unused-expressions": "off",
				"no-explicit-any": "off",
				"no-identical-title": "error",
				"prefer-dom-node-text-content": "error",
				"typescript/no-non-null-assertion": "off",
				"valid-expect": "off",
				"valid-title": "off",
				"vitest/no-import-node-test": "error"
			}
		},
		{
			"files": ["src/utils/**/*.ts"],
			"rules": {
				"typescript/explicit-function-return-type": "error"
			}
		},
		{
			"files": ["*.config.{ts,js}"],
			"rules": {
				"import/no-default-export": "off"
			}
		}
	]
}
