{
	"name": "sudoku",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check:format": "prettier --check --log-level=silent --list-different src/",
		"check:lint": "run-s lint:*",
		"check:types": "vue-tsc --build",
		"fix:format": "prettier --list-different src/",
		"fix:lint": "run-s fix:lint:*",
		"fix:lint:eslint": "eslint ./src --fix",
		"fix:lint:oxlint": "oxlint ./src --fix --fix-suggestions --ignore-path .gitignore",
		"lint:eslint": "eslint .",
		"lint:oxlint": "oxlint ./src --ignore-path .gitignore",
		"start:dev": "vite",
		"start:build": "vite preview",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest"
	},
	"dependencies": {
		"pinia": "^2.3.0",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.13.1",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitejs/plugin-vue-jsx": "^4.1.1",
		"@vitest/eslint-plugin": "^1.1.28",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.4.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "^9.20.0",
		"eslint-plugin-oxlint": "^0.15.10",
		"eslint-plugin-vue": "^9.32.0",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^7.0.1",
		"oxlint": "^0.15.10",
		"prettier": "^3.3.3",
		"typescript": "~5.7.3",
		"vite": "^6.1.0",
		"vite-plugin-vue-devtools": "^7.6.8",
		"vitest": "^3.0.5",
		"vue-tsc": "^2.2.0"
	},
	"packageManager": "npm@11.1.0+sha512.acf301ad9b9ddba948fcb72341e2f0fcae477f56a95cc2a092934d133a7461062633cefbf93d5934a3dc0768674e2edee9f04dcfcc4bb4c327ff0e3a7d552a1b"
}
