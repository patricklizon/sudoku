{
	"name": "sudoku",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check:format": "prettier --check --log-level=silent --list-different src/",
		"check:lint": "run-s lint:*",
		"check:types": "vue-tsc --build",
		"fix:format": "prettier --list-different src/",
		"fix:lint": "run-s fix:lint:*",
		"fix:lint:eslint": "eslint . --fix",
		"fix:lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
		"lint:eslint": "eslint .",
		"lint:oxlint": "oxlint . -D correctness --ignore-path .gitignore",
		"start:dev": "vite",
		"start:build": "vite preview",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest"
	},
	"dependencies": {
		"pinia": "^2.3.0",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
	},
	"devDependencies": {
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.10.2",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitejs/plugin-vue-jsx": "^4.1.1",
		"@vitest/eslint-plugin": "^1.1.28",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.1.3",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "^9.20.0",
		"eslint-plugin-oxlint": "^0.15.10",
		"eslint-plugin-vue": "^9.32.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"oxlint": "^0.15.10",
		"prettier": "^3.3.3",
		"typescript": "~5.6.3",
		"vite": "^6.0.5",
		"vite-plugin-vue-devtools": "^7.6.8",
		"vitest": "^2.1.8",
		"vue-tsc": "^2.1.10"
	},
	"packageManager": "npm@10.9.0+sha512.65a9c38a8172948f617a53619762cd77e12b9950fe1f9239debcb8d62c652f2081824b986fee7c0af6c0a7df615becebe4bf56e17ec27214a87aa29d9e038b4b"
}
