{
	"name": "sudoku",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vinxi build",
		"generate:cf-types": "wrangler types @types/generated/worker-configuration.d.ts",
		"postgenerate:cf-types": "[ \"$CI\" != \"true\" ] && exit 0; git diff --quiet @types/generated/worker-configuration.d.ts || (echo 'worker-configuration.d.ts is outdated' && exit 1)",
		"generate:diagrams": "d2 docs/diagrams/architecture-layers.d2 docs/diagrams/.output/architecture-layers.svg",
		"check:format": "prettier --check .",
		"check:lint": "run-s lint:*",
		"check:cf-types": "bun run generate:cf-types",
		"check:types": "tsc --noEmit",
		"dev": "NODE_ENV=dev vinxi dev --port 5173",
		"fix:format": "prettier . --list-different --write",
		"fix:lint": "run-s fix:lint:*",
		"fix:lint:eslint": "eslint ./src --fix",
		"fix:lint:oxlint": "oxlint ./src --fix --fix-suggestions --ignore-path .gitignore",
		"lighthouse": "lhci autorun --collect.staticDistDir=\"./build\"",
		"lint:eslint": "eslint \"src/**/*.ts\" \"src/**/*.tsx\" \"e2e/**/*.ts\" \"test/**/*.ts\"",
		"lint:oxlint": "oxlint --ignore-path .gitignore",
		"preview": "wrangler dev --local --port 8787 --log-level error",
		"start": "vinxi start",
		"test:browser": "vitest --project browser",
		"test:browser:run": "vitest --project browser --run",
		"test:e2e": "playwright test",
		"test:e2e:dev": "playwright test --ui --config=playwright.dev.config.ts",
		"test:install:browsers": "playwright install chromium",
		"test:install:deps": "playwright install-deps chromium",
		"test:unit": "vitest --project unit",
		"test:unit:run": "vitest --project unit --run",
		"wrangler": "wrangler"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@faker-js/faker": "^9.9.0",
		"@lhci/cli": "^0.15.1",
		"@playwright/test": "^1.54.2",
		"@solidjs/testing-library": "^0.8.10",
		"@tsconfig/node22": "^22.0.2",
		"@types/bun": "^1.2.19",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.2.1",
		"@vitest/browser": "^3.2.4",
		"@vitest/eslint-plugin": "^1.1.28",
		"@vitest/ui": "3.2.4",
		"dotenv": "^17.2.1",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-oxlint": "^1.9.0",
		"eslint-plugin-playwright": "^2.2.2",
		"eslint-plugin-solid": "^0.14.5",
		"globals": "^16.3.0",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^8.0.4",
		"oxlint": "^1.9.0",
		"prettier": "^3.6.2",
		"sinon": "^21.0.0",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.39.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"wrangler": "^4.27.0"
	},
	"dependencies": {
		"@solidjs/meta": "^0.29.4",
		"@solidjs/router": "^0.15.3",
		"@solidjs/start": "^1.1.7",
		"solid-js": "^1.9.9",
		"vinxi": "^0.5.8"
	}
}
