{
  "name": "sudoku",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check:format": "prettier ./src --check",
    "check:lint": "run-s lint:*",
    "check:types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.app.json",
    "check:types:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.app.json --watch",
    "fix:format": "prettier ./src ./tests --list-different --write",
    "fix:lint": "run-s fix:lint:*",
    "fix:lint:eslint": "eslint ./src --fix",
    "fix:lint:oxlint": "oxlint ./src --fix --fix-suggestions --ignore-path .gitignore",
    "install-browsers": "npx playwright install chromium",
    "lighthouse": "lhci autorun --collect.staticDistDir=\"./build\"",
    "lint:eslint": "eslint .",
    "lint:oxlint": "oxlint ./src --ignore-path .gitignore",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview",
    "start:build": "vite preview",
    "start:dev": "vite",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --config=playwright.dev.config.ts",
    "test:e2e:dev:ui": "playwright test --ui --config=playwright.dev.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.51.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.17.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/svelte": "^5.2.7",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@vitest/eslint-plugin": "^1.1.28",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-oxlint": "^0.15.10",
    "eslint-plugin-svelte": "^3.0.2",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.15.13",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.20.5",
    "svelte-check": "^4.1.4",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
