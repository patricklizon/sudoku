{
	"name": "sudoku",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vinxi build",
		"cf-typegen": "wrangler types src/worker-configuration.d.ts",
		"check:format": "prettier --check .",
		"check:lint": "run-s lint:*",
		"check:types": "tsc --noEmit",
		"deploy": "echo 'install a server adapter'",
		"dev": "vinxi dev",
		"dev:cf": "wrangler dev --local",
		"fix:format": "prettier . --list-different --write",
		"fix:lint": "run-s fix:lint:*",
		"fix:lint:eslint": "eslint ./src --fix",
		"fix:lint:oxlint": "oxlint ./src --fix --fix-suggestions --ignore-path .gitignore",
		"install-browsers": "playwright install chromium",
		"install:browsers": "playwright install chromium",
		"lighthouse": "lhci autorun --collect.staticDistDir=\"./build\"",
		"lint:eslint": "eslint \"src/**/*.ts*\" \"tests/**/*.ts*\"",
		"lint:oxlint": "oxlint \"src/**/*.ts*\" \"test/**/*.ts*\" --ignore-path .gitignore",
		"preview": "wrangler dev --local --port 3000 --log-level error",
		"start": "vinxi start",
		"start:dev": "vite",
		"test": "vitest --project browser unit",
		"test:browser": "vitest --project browser",
		"test:browser:run": "vitest --project browser --run",
		"test:e2e": "playwright test",
		"test:e2e:dev": "playwright test --config=playwright.dev.config.ts",
		"test:e2e:dev:ui": "playwright test --ui --config=playwright.dev.config.ts",
		"test:e2e:ui": "playwright test --ui",
		"test:unit": "vitest run",
		"test:unit:run": "vitest --project unit --run",
		"test:unit:watch": "vitest",
		"wrangler": "wrangler"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@lhci/cli": "^0.14.0",
		"@playwright/test": "^1.51.1",
		"@solidjs/testing-library": "^0.8.10",
		"@tsconfig/node22": "^22.0.0",
		"@types/bun": "^1.2.19",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.13.1",
		"@vitest/eslint-plugin": "^1.1.28",
		"eslint": "^9.21.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-oxlint": "^0.15.10",
		"eslint-plugin-playwright": "^2.2.2",
		"eslint-plugin-solid": "^0.14.5",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^7.0.1",
		"oxlint": "^0.15.13",
		"prettier": "^3.3.3",
		"typescript": "~5.7.3",
		"typescript-eslint": "^8.26.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.0.5",
		"wrangler": "^4.27.0"
	},
	"dependencies": {
		"@solidjs/meta": "^0.29.4",
		"@solidjs/router": "^0.15.3",
		"@solidjs/start": "^1.1.7",
		"solid-js": "^1.9.7",
		"vinxi": "^0.5.8"
	}
}
