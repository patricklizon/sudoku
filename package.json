{
	"name": "sudoku",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:preview": "vite preview",
		"check:format": "prettier --check --log-level=silent --list-different src/",
		"check:lint": "run-s lint:*",
		"check:types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.app.json",
		"check:types:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.app.json --watch",
		"fix:format": "prettier --list-different src/",
		"fix:lint": "run-s fix:lint:*",
		"fix:lint:eslint": "eslint ./src --fix",
		"fix:lint:oxlint": "oxlint ./src --fix --fix-suggestions --ignore-path .gitignore",
		"lint:eslint": "eslint .",
		"lint:oxlint": "oxlint ./src --ignore-path .gitignore",
		"prepare": "svelte-kit sync || echo ''",
		"start:build": "vite preview",
		"start:dev": "vite",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/svelte": "^5.2.7",
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.13.1",
		"@vitest/eslint-plugin": "^1.1.28",
		"eslint": "^9.20.0",
		"eslint-config-prettier": "^10.0.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-oxlint": "^0.15.10",
		"eslint-plugin-svelte": "^3.0.2",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"npm-run-all2": "^7.0.1",
		"oxlint": "^0.15.10",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"typescript": "~5.7.3",
		"typescript-eslint": "^8.26.0",
		"vite": "^6.1.0",
		"vitest": "^3.0.5"
	},
	"packageManager": "npm@11.1.0+sha512.acf301ad9b9ddba948fcb72341e2f0fcae477f56a95cc2a092934d133a7461062633cefbf93d5934a3dc0768674e2edee9f04dcfcc4bb4c327ff0e3a7d552a1b"
}
